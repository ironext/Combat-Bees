
.query-view__empty {
    margin: 0;
    padding: 3px;
    border-color: var(--section-border-bottom-color);
    border-bottom-width: 1px;
    -unity-font-style: italic;
}

.query-view__open-query-window-button {
    background-color: transparent;
    border-color: transparent;
    background-image: var(--open-in-window-icon);
    width:16px;
    height:16px;
    flex-shrink: 0;
    flex-grow: 0;
}
