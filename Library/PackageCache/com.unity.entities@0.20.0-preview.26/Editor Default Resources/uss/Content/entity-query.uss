CustomInspectorElement, PropertyElement, .entity-query__container {
    flex-grow: 1;
    flex-shrink: 0;
}

.entity-query__search-element-container
{
    margin: 3px;
}

.entity-query__list-view
{
    flex-grow: 1;
}

.entity-query-foldout__container {
    flex-direction: row;
    flex-grow: 1;
    -unity-font-style: bold;
    padding: 3px 3px 3px 0;
}

.entity-query-foldout__system-icon {
    width: 16px;
    height: 16px;
    margin-right: 5px;
    flex-shrink: 0;
    flex-grow: 0;
    background-image: var(--system-icon);
}

.entity-query-foldout__query-icon {
    width: 16px;
    height: 16px;
    margin-left: 5px;
    margin-right: 5px;
    flex-shrink: 0;
    flex-grow: 0;
    background-image: var(--query-icon);
    overflow: hidden;
}

.entity-query-foldout__entity-count { margin-left: 5px; }
.entity-query-foldout__system-name { flex-grow: 0; }
.entity-query-foldout__query-name { flex-grow: 1; }
.entity-query-foldout__system-name, .entity-query-foldout__query-name {
    margin: 0;
    padding: 0;
    flex-shrink: 1;
    min-width: 0;
    overflow: hidden;
    text-overflow: ellipsis;
}

.entity-query-foldout__empty {
    margin: 0;
    padding: 3px;
    border-color: var(--section-border-bottom-color);
    border-bottom-width: 1px;
    -unity-font-style: italic;
}

FoldoutWithHeader > Toggle {
    margin: 0;
    padding: 0;
}

FoldoutWithHeader > Toggle {
    border-color: var(--section-border-bottom-color);
    border-bottom-width: 1px;
    margin: 0;
    padding: 0;
}

FoldoutWithHeader > Toggle > .unity-toggle__input {
    padding-top: 3px;
}

FoldoutWithHeader > Toggle:hover {
    background-color: var(--header-background-hover-color);
}


